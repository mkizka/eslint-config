{
  "name": "@mkizka/eslint-config",
  "version": "5.3.4",
  "type": "module",
  "description": "ESLint rules for mkizka",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkizka/eslint-config.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "pnpm -s lint --fix && prettier --write .",
    "lint": "pnpm -s build && eslint .",
    "prepare": "husky",
    "preview": "pnpm -s dlx @eslint/config-inspector@latest",
    "release": "pnpm -s build && np",
    "test": "pnpm -s build && vitest run"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@dword-design/eslint-plugin-import-alias": "^5.1.1",
    "@eslint/js": "^9.14.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.11.0",
    "jsonc-parser": "^3.3.1",
    "typescript-eslint": "^8.12.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.8.7",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "prettier-plugin-pkg": "^0.18.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.ts": "eslint --fix"
  }
}

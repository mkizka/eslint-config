  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "np": "^10.0.2",
    "prettier": "^3.2.5",

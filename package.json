{
  "name": "@mkizka/eslint-config",
  "version": "5.5.0",
  "type": "module",
  "description": "ESLint rules for mkizka",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkizka/eslint-config.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "pnpm -s lint --fix && prettier --write .",
    "lint": "pnpm typecheck && eslint .",
    "prepare": "husky",
    "preview": "pnpm -s dlx @eslint/config-inspector@latest",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@dword-design/eslint-plugin-import-alias": "^5.1.1",
    "@eslint/js": "^9.22.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jsonc-parser": "^3.3.1",
    "typescript-eslint": "^8.27.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@tsconfig/node-lts": "^22.0.1",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-pkg": "^0.18.1",
    "tsup": "^8.4.0",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.ts": "eslint --fix"
  }
}
